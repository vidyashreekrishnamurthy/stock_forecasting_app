class Cloth
{
String cloth_color;
int cloth_cost;
char cloth_size;
Cloth(String a,int b,char c)
{
cloth_color=a;
cloth_cost=b;
cloth_size=c;
return;
}
public static void main(String[] args)
{
Cloth C1= new Cloth("red",50000,'M');
System.out.println(C1.cloth_color);
System.out.println(C1.cloth_cost);
System.out.println(C1.cloth_size);
}
}

